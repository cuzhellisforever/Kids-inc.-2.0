        <img src="data:image/jpeg;base64," alt="painting">
        <div class="text">kids inc, releasing printing press!</div>
        </div>
        <a class="prev" onclick="nextpicture(-1)">8#10094;</a>
        <a class="next" onclick="nextpicture(1)">8#10095;</a>
        <div style="text-align: center;">
            <div class="dot" onclick="currentpicture(1)"></div>
            <div class="dot" onclick="currentpicture(2)"></div>
            <div class="dot" onclick="currentpicture(3)"></div>
        </div>
    </div>
